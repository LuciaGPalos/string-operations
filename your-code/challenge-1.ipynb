   "source": [
    "str_list = ['Durante', 'un', 'tiempo', 'no', 'estuvo', 'segura', 'de', 'si', 'su', 'marido', 'era', 'su', 'marido']\n",
    "# Your code here:\n"
    #Add period at end
    str_list.append(".")

    #Joining all strings with space:
    str_list_space=' '.join(str_list)
    str(str_list_space).replace('marido .','marido.')
